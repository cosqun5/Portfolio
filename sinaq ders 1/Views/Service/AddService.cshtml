@model EntityLayer.Concrate.Service

@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card-header border-0">
    <h3 class="mb-0">Xidmet Ekleme Səhifəsi</h3>
</div>

@using (Html.BeginForm("AddService", "Service", FormMethod.Post))
{

    <br />
    @Html.Label("Title")
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    @Html.Label("ImageUrl")
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-info">Kaydet</button>

}

