@model sinaq_ders_1.Areas.Writer.Models.UserRegistrViewModel
@{
    Layout = null;
}






<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Skydash Admin</title>
    <link rel="stylesheet" href="~/skydash/vendors/feather/feather.css">
    <link rel="stylesheet" href="~/skydash/vendors/ti-icons/css/themify-icons.css">
    <link rel="stylesheet" href="~/skydash/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/skydash/css/vertical-layout-light/style.css">
    <link rel="shortcut icon" href="~/skydash/images/favicon.png" />
</head>
//123456aA*
<body>
    <div class="container-scroller">
        <div class="container-fluid page-body-wrapper full-page-wrapper">
            <div class="content-wrapper d-flex align-items-center auth px-0">
                <div class="row w-100 mx-0">
                    <div class="col-lg-4 mx-auto">
                        <div class="auth-form-light text-left py-5 px-4 px-sm-5">
                            <div class="brand-logo">
                                <img src="~/skydash/images/logo.svg" alt="logo">
                            </div>
                            <h4>Bir hesabin yoxdu?</h4>
                            <h6 class="font-weight-light">2 Deqiqeye Qeydiyyatdan kec</h6>
                            <form class="pt-3" method="post">
                                <div class="form-group">
                                    <label>Adiniz</label>
                                    <span asp-validation-for="Name" class="text-danger"></span>
                                    <input type="text" asp-for="Name" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Soaydiniz</label>
                                    <span asp-validation-for="Surname" class="text-danger"></span>
                                    <input type="text" asp-for="Surname" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Kullanici Adiniz </label>
                                    <span asp-validation-for="UserName" class="text-danger"></span>
                                    <input type="text" asp-for="UserName" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>ImageUrl</label>
                                    <span asp-validation-for="ImageUrl" class="text-danger"></span>
                                    <input type="text" asp-for="ImageUrl" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Sifre</label>
                                    <span asp-validation-for="Password" class="text-danger"></span>
                                    <input type="text" asp-for="Password" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Sifreni tekrar yaz</label>
                                    <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                                    <input type="text" asp-for="ConfirmPassword" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Email</label>
                                    <span asp-validation-for="Mail" class="text-danger"></span>
                                    <input type="text" asp-for="Mail" class="form-control" />
                                </div>
                                <div class="text-danger" asp-validation-summary="ModelOnly"></div>

                                <div class="mt-3">
                                    <button class="btn btn-primary">Qeydiyyatdan Kec</button>
                                    @*<a class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn" href="~/skydash/index.html">SIGN UP</a>*@
                                </div>
                                <div class="text-center mt-4 font-weight-light">
                                    Already have an account? <a href="login.html" class="text-primary">Login</a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/skydash/vendors/js/vendor.bundle.base.js"></script>

    <script src="~/skydash/js/off-canvas.js"></script>
    <script src="~/skydash/js/hoverable-collapse.js"></script>
    <script src="~/skydash/js/template.js"></script>
    <script src="~/skydash/js/settings.js"></script>
    <script src="~/skydash/js/todolist.js"></script>

</body>

</html>






































